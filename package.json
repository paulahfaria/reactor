{
  "name": "reactor",
  "version": "1.0.0",
  "description": "Universal React boilerplate using Express, Webpack, React Router, React, React Helmet, Glamor and Redux.",
  "main": "src/server.js",
  "repository": "git@github.com:obetomuniz/reactor.git",
  "scripts": {
    "dev": "npm run dev-hmr",
    "dev-single-page": "node_modules/.bin/http-server src/static",
    "dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "prod": "npm run build && NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015,stage-0' src/server.js",
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015,stage-0' src/server.js",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p",
    "webpack-analyzer": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json ./",
    "test": "jest"
  },
  "author": "Beto Muniz",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "ejs": "^2.5.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.0",
    "express": "^4.14.0",
    "glamor": "^2.18.2",
    "http-server": "^0.9.0",
    "jest": "^17.0.3",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.2.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.6",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
